<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewListName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/listName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewPriceItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/createdAt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewListName" />

    <TextView
        android:id="@+id/textViewQntItems"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginLeft="108dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/qntItems"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textViewPriceItem"
        app:layout_constraintTop_toBottomOf="@+id/textViewListName" />

    <TextView
        android:id="@+id/textViewPriceValue"
        android:layout_width="90dp"
        android:layout_height="18dp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="52dp" />

    <TextView
        android:id="@+id/textViewQntValue"
        android:layout_width="71dp"
        android:layout_height="19dp"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="51dp" />

    <TextView
        android:id="@+id/textViewNameValue"
        android:layout_width="335dp"
        android:layout_height="18dp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="15dp" />
</androidx.constraintlayout.widget.ConstraintLayout>